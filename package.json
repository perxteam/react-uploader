{
  "name": "react-uploader",
  "version": "0.0.1",
  "description": "Files upload widget for React",
  "scripts": {
    "build:builder": "webpack -p",
    "build:viewer": "cross-env APP_TYPE=viewer webpack -p",
    "dev:viewer": "cross-env APP_TYPE=viewer webpack-dev-server --progress",
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ app/ && cp -R app/assets lib/ && git add .",
    "build": "concurrently \"npm run build:builder\" \"npm run build:viewer\"",
    "start": "webpack-dev-server --progress --profile --colors"
  },
  "pre-commit": [
    "build:lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:perxteam/form-service.git"
  },
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "isomorphic-fetch": "^2.2.1",
    "promise-polyfill": "^6.0.2",
    "ramda": "^0.23.0",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-hot-loader": "^3.0.0-beta.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
